<IfModule mod_rewrite.c>
  RewriteEngine On RewriteBase / # If the request is for a file or directory
  that exists, serve it directly RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d RewriteRule ^ - [L] # If the request is not
  for an existing file or directory, rewrite to index.html RewriteRule .
  /index.html [L]
</IfModule>
